{
	"masterPort": 8090,
	"masterHost": "com.breizbot.ovh",
	"masterUser": "user",
	"masterPwd": "pwd",

	"launcher": {
		"logPath": "./logs",

		"agents": {
			"shapeAgent": {"script": "agents/shapeAgent.js", "start": "manual"},			
			"drone": {"script": "agents/drone.js", "start": "manual"},
			"radar": {"script": "agents/radar.js", "start": "manual"},
			"sumProvider": {"script": "agents/test/service/provider.js", "start": "manual"},
			"parrot": {"script": "agents/parrot.js", "start": "manual"},
			"tplink": {"script": "agents/tplink.js", "start": "manual"},
			"arduino": {"script": "agents/arduino/index.js", "start": "manual"}
		}
	},

	"drone": {
		"storeInFile": false,
		"storeFileName": "bus2.json",
		"warpFactor": 10,
		"gyrSpeed": 8,
		"captureRadius": 10,
		"icon": {
		 	"type": "ais"
		}		
	},

	"arduino": {
		"devices": {
			"led1": {
				"type": "rgbLed",
				"alias": "RGB Led",
				"options": {
					"pins": {
						"red": 5,
						"green": 6,
						"blue": 3
					}
				}
			},
			"led2": {
				"type": "led",
				"alias": "Led Rouge",
				"options": {
					"pin": 13
				}
			},
			"button1": {
				"type": "pushButton",
				"alias": "Push button",
				"options": {
					"pin": 2
				}
			}
		}
	}


}
